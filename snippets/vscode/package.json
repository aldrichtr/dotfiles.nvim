{
    "name": "aldrichtr-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "filetype/global.jsonc"
            },
            {
                "language": [
                    "lua"
                ],
                "path": "filetype/lua.jsonc"
            },
            {
                "language": [
                    "powershell",
                    "ps1"
                ],
                "path": "filetype/powershell.jsonc"
            }
        ]
    }
}
